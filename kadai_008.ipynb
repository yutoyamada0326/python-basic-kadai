        "import random\n",
        "# 変数numに0～4までのランダムな整数を代入する\n",
        "var = random.randint(0, 99)\n",
        "\n",
        "if var % 3 == 0 :\n",
        "    print(\"Fizz\")\n",
        "elif var % 5 == 0:\n",
        "    print(\"Buzz\")\n",
        "elif var % 15 == 0 :\n",
        "    print(\"FizzBuzz\")\n",
        "else:\n",
        "    print(var)\n"
      ]
    }
